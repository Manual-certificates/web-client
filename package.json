{
  "name": "vue-vite-template",
  "version": "0.0.0",
  "gitHooks": {
    "pre-commit": "yarn lint",
    "pre-push": "yarn test && yarn rsc"
  },
  "scripts": {
    "vite-dev-server": "vite",
    "vite-build": "vite build",
    "start": "cross-env VITE_ENVIRONMENT=development yarn vite-dev-server",
    "build": "cross-env VITE_ENVIRONMENT=production yarn vite-build",
    "analyze": "cross-env VITE_ENVIRONMENT=analyze yarn vite-build",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "start-server-and-test start http-get://localhost:8095 cypress",
    "test:unit": "jest --passWithNoTests src",
    "cypress": "cypress run",
    "lint": "yarn lint:styles && yarn lint:scripts",
    "lint:styles": "stylelint \"src/**/*.{vue,scss,css}\" --max-warnings=0",
    "lint:scripts": "eslint \"{src,config}/**/*.{vue,js}\" --cache --fix --max-warnings=0",
    "preview": "vite preview",
    "rsc": "node scripts/release-sanity-check.mjs"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.0-alpha.34",
    "@vuelidate/validators": "^2.0.0-alpha.26",
    "axios": "^0.26.0",
    "bignumber.js": "^9.0.2",
    "dayjs": "^1.10.7",
    "jsona": "^1.9.6",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.0",
    "mitt": "^3.0.0",
    "pinia": "^2.0.11",
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.0-beta.26",
    "vue-router": "4",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
    "@types/jest": "^27.4.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.9",
    "@types/vuelidate": "^0.7.15",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "chalk": "^5.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.2.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "^1.49.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.2.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-declaration-strict-value": "^1.8.0",
    "stylelint-scss": "^4.1.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-checker": "^0.3.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "yorkie": "^2.0.0"
  }
}
