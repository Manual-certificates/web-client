{
  "name": "vue-vite-template",
  "version": "0.0.0",
  "gitHooks": {
    "pre-commit": "yarn lint",
    "pre-push": "yarn test && yarn rsc"
  },
  "scripts": {
    "start": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "start-server-and-test start http-get://localhost:3000 cypress",
    "test:unit": "jest src",
    "cypress": "cypress run",
    "lint": "yarn lint:styles && yarn lint:scripts",
    "lint:styles": "stylelint \"src/**/*.{vue,scss,css}\" --max-warnings=0",
    "lint:scripts": "eslint \"{src,config}/**/*.{vue,js}\" --cache --fix --max-warnings=0",
    "preview": "vite preview",
    "rsc": "node scripts/release-sanity-check.mjs"
  },
  "dependencies": {
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.0-beta.26"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "chalk": "^5.0.0",
    "eslint": "^8.7.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "jest": "^27.4.7",
    "stylelint": "^14.2.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-declaration-strict-value": "^1.8.0",
    "stylelint-scss": "^4.1.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8",
    "yorkie": "^2.0.0"
  }
}
