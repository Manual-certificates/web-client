{
  "name": "vue-vite-template",
  "version": "1.0.0-rc.1",
  "gitHooks": {
    "pre-commit": "yarn lint",
    "pre-push": "yarn test && yarn rsc"
  },
  "scripts": {
    "vite-dev-server": "vite",
    "vite-build": "vite build",
    "start": "cross-env VITE_APP_ENVIRONMENT=development yarn vite-dev-server",
    "build": "cross-env VITE_APP_ENVIRONMENT=production yarn vite-build",
    "analyze": "cross-env VITE_APP_ENVIRONMENT=analyze yarn vite-build",
    "test": "yarn test:unit",
    "test:unit": "vitest run",
    "lint": "yarn lint:styles && yarn lint:scripts",
    "lint:styles": "stylelint \"src/**/*.{vue,scss,css}\" --max-warnings=0",
    "lint:scripts": "eslint \"{src,config}/**/*.{vue,js,ts}\" --cache --fix --max-warnings=0",
    "preview": "vite preview",
    "rsc": "node scripts/release-sanity-check.mjs"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "@vueuse/core": "^9.9.0",
    "axios": "^1.2.1",
    "bignumber.js": "^9.1.1",
    "dayjs": "^1.11.7",
    "jsona": "^1.11.0",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.1",
    "mitt": "^3.0.0",
    "pinia": "^2.0.28",
    "postcss": "^8.4.20",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.17",
    "@types/vuelidate": "^0.7.15",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "chalk": "^5.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.8.0",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "happy-dom": "^8.1.1",
    "prettier": "^2.8.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.57.1",
    "stylelint": "^14.16.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-declaration-strict-value": "^1.8.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-checker": "^0.5.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.26.2",
    "vue-eslint-parser": "^9.0.2",
    "yorkie": "^2.0.0"
  }
}
